{
  "name": "gfdp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.14",
    "pug": "^3.0.2",
    "socketio": "^1.0.0",
    "socketio-jwt": "^4.6.2",
    "uuid": "^8.3.2"
  },
  "routers": {
    "index": {
      "alias":"/",
      "folder": "",
      "routes":[
        {
          "path": "/",
          "method": "get",
          "name": "Express",
          "page": "index",
          "data": {
            "page": "signin"
          }
        },
        {
          "path": "/home",
          "method": "get",
          "name": "Express",
          "page": "index",
          "data": {
            "home": true
          }
        },
        {
          "path": "/inscription",
          "method": "get",
          "name": "Express",
          "page": "index",
          "data": {
            "page": "signup"
          }
        },
        {
          "path": "/logged",
          "method": "get",
          "name": "Express",
          "page": "index",
          "data": {
            "logged": true
          }
        }
      ]
    },
    "api": {
      "alias": "/store",
      "folder": "/store",
      "routes":[
        {
          "path": "/*",
          "method": "get",
          "action": "read"
        },
        {
          "path": "/*",
          "method": "post",
          "action": "create"
        },
        {
          "path": "/*",
          "method": "patch",
          "action": "update"
        },
        {
          "path": "/*",
          "method": "delete",
          "action": "delete"
        }
      ]
    },
    "users": {
      "alias": "",
      "folder": "",
      "routes":[
        {
          "path": "/login",
          "methods": [
            "createToken"
          ],
          "data": {
            "req": [
              "nom",
              "mdp",
              "email"
            ]
          }

        }
      ]
    }
  },
  "ressources": [

  ]
}
